@model RegUser
<h1>Register!</h1>
<form asp-action='Register' asp-controller='Home' method='post'>
    <div id='form_group'>
        <span asp-validation-for='first_name'></span>
        <label asp-for='first_name'></label>
        <input asp-for='first_name'>
    </div>
    <div id="form_group">
        <span asp-validation-for='last_name'></span>
        <label asp-for='last_name'></label>
        <input asp-for='last_name'>
    </div>
    <div id="form_group">
        <span asp-validation-for='email'></span>
        <label asp-for='email'></label>
        <input asp-for='email'>
    </div>
    <div id="form_group">
        <span asp-validation-for='password'></span>
        <label asp-for='password'></label>
        <input asp-for='password'>
    </div>
    <div id="form_group">
        <span asp-validation-for='confirm'></span>
        <label asp-for='confirm'></label>
        <input asp-for='confirm'>
    </div>
    <input type='submit' value='submit'>
</form>

<a href="Log">Already Registered?</a>

